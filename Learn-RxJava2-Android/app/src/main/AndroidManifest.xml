<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.watanabear.learn_rxjava2_android">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".presentation.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presentation.rxbus.RxBusActivity" />
        <activity android:name=".presentation.pagination.PaginationActivity" />
        <activity android:name=".presentation.OperatorsActivity" />
        <activity android:name=".presentation.operators.SimpleActivity"/>
        <activity android:name=".presentation.operators.MapActivity"/>
        <activity android:name=".presentation.operators.ZipActivity"/>
        <activity android:name=".presentation.operators.DisposableActivity"/>
        <activity android:name=".presentation.operators.TakeActivity"/>
        <activity android:name=".presentation.operators.TimerActivity"/>
    </application>

</manifest>